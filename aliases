# General aliases
alias psaux='ps aux'
alias tailf='tail -f '

# Git aliases
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit '
alias gd='git diff '
alias go='git checkout '
alias gk='gitk --all&'
alias gx='gitx --all'

# Forever aliases
alias fl='forever list'
alias fr='forever restart'
alias fra='forever restartall'
alias fs='forever stop'
alias fsa='forever stopall'
